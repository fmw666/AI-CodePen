{
    "version": 2,
    "builds": [
        { "src": "src/main.py", "use": "@vercel/python" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/src/main.py" },
        { "src": "/chatgpt", "methods": ["POST"], "dest": "/src/main.py" }
    ],
    "env": {
      "PYTHONUNBUFFERED": "1"
    }
}
